<h1>Bloggers#new</h1>
<% if @blogger.errors %>
<p>There were some errors:</p><br></br>
<% @blogger.errors.full_messages.each do |message| %>
<li><%= message %></li>
<% end %>
<% end %>

<%= form_for @blogger do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :bio %>
<%= f.text_area :bio %>

<%= f.label :age %>
<%= f.number_field :age %>

<%= f.submit %>
<% end %>
